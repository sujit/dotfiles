{
    "editor.fontFamily": "MonoLisaNerdFontCompleteM-Regular,VictorMonoNerdFontCompleteM-Medium,JetBrainsMonoNerdFontCompleteM-Regular,OperatorMonoNerdFontComplete-Medium,ComicCode Nerd Font,Iosevka Term SS04,OperatorMonoSSm Nerd Font,M PLUS Code Latin,Iosevka Medium Nerd Font Complete Mono,Iosevka Nerd Font Mono,CamingoCode Nerd Font,OperatorMonoSSm Nerd Font,Recursive Code iCursive Cg,Hack,Sarasa NF,MonoLisaRegular NF,agave Nerd Font Mono,Sarasa NF,Fira Code Retina,Menlo, Monaco, 'Courier New', monospace",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "diffEditor.wordWrap": "on",
    "diffEditor.diffAlgorithm":"advanced",
    "editor.autoClosingBrackets": "always",
    "editor.autoIndent": "full",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeLens": false,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "block",
    "editor.detectIndentation": true,
    "editor.fontLigatures": true,
    "editor.fontWeight": "500",
    "editor.formatOnPaste": true,
    "editor.guides.highlightActiveIndentation": true,
    "editor.guides.indentation": true,
    "editor.insertSpaces": true,
    "editor.largeFileOptimizations": false,
    "editor.guides.bracketPairs": true,
    "editor.lineHeight": 0,
    "editor.maxTokenizationLineLength": 200000,
    "editor.mouseWheelZoom": true,
    "editor.parameterHints.enabled": true,
    "editor.rulers": [],
    "editor.smoothScrolling": false,
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "editor.wordWrap": "on",
    "explorer.sortOrder": "filesFirst",
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": false,
    "editor.hideCursorInOverviewRuler": false,
    "files.exclude": {
        "**/__history/**": true,
        "**/__recovery/**": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/*.{res,RES}": true,
        "**/*.bak?": true,
        "**/*.bpl": true,
        "**/*.dcu": true,
        "**/*.dll": true,
        "**/*.ico": true,
        "**/*.local": true,
        "**/*.map": true,
        "**/*.rc": true,
        "**/*.rsb": true,
        "**/*.tds": true,
        "**/*.tmp": true,
        "**/CVS": true,
        "**/Lib/site-packages": true,
        "**/Win32/**": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": false,
    "git.enabled": true,
    "json.maxItemsComputed": 10000,
    "partialDiff.enableTelemetry": false,
    "python.envFile": "${workspaceFolder}/.env",
    "python.formatting.provider": "yapf",
    "python.linting.enabled": true,
    "python.linting.pylintArgs": [
        "--linter=ruff"
    ],
    "python.terminal.activateEnvironment": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontFamily": "Recursive Code iCursive Cg",
    "terminal.integrated.fontSize": 11,
    "window.title": "${rootPath}${separator}${appName}",
    "window.titleBarStyle": "custom",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "sweet-vscode-icons",
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.untitled.hint": "hidden",
    "security.workspace.trust.untrustedFiles": "open",
    "vscode-json-editor.theme": "dark",
    "workbench.layoutControl.enabled": false,
    "editor.stickyScroll.enabled": true,
    "python.defaultInterpreterPath": "/Users/sujit/tools/pcap_packet_fu/.venv",
    "editor.tabCompletion": "on",
    "editor.acceptSuggestionOnEnter": "on",
    "terminal.integrated.tabStopWidth": 4,
    "workbench.colorTheme": "Gruvbox Material Dark",
    "window.zoomLevel": 1
}
